<!doctype html>
<html>
<head>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">

  <!-- include system scripts, page meta, and header code injection -->
  {squarespace-headers}

</head>

<body id="{squarespace.page-id}" class="{squarespace.page-classes}">

  <div class="site-container">

    <!-- Navbar -->
    <header>
      {@|apply menu.block}
      {@|apply navbar.block}
    </header>

    <main class="content-container" role="main" data-content-field="main-content">

      <!-- Hero image -->
      <div class="hero__video">
        <div class="util__z-above-overlay text-container ">
          <div class="page__title">
            <squarespace:block-field id="workshop__hero-title" columns="12" lock-layout="true"/>
          </div>
          <div class="page__headline util__text-red">
            <squarespace:block-field id="workshop__hero-header" columns="12" lock-layout="true"/>
          </div>
          <div class="page__subheadline util__text-white">
            <squarespace:block-field id="workshop__hero-description" columns="12" lock-layout="true"/>
          </div>
        </div>
        <video autoplay="autoplay" loop="loop" muted="muted" playsinline="playsinline" class="util__grayscale">
          <source src="http://foundrymakes.com/uploads/video/gzMNwbmRsKsewX3Y.webm" type="video/webm"/>
          <source src="http://foundrymakes.com/uploads/video/sgWoFYRIh2_DKjDE.mp4" type="video/mp4"/>
        </video>
        <div class="util__overlay-blue"></div>
      </div>

