{
  "name": "foundrymakes-ss",
  "version": "1.0.0",
  "description": "a Squarespace template for Foundry",
  "main": "site.region",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bs": "browser-sync start --proxy 'http://localhost:9000' --reload-delay 2000 --no-open --files 'collections' 'blocks' 'assets' 'scripts' 'styles' '*.region'",
    "dev": "parallelshell 'squarespace-server https://foundrymakes-ss.squarespace.com --auth' 'npm run bs'"
  },
  "repository": {
    "type": "git",
    "url": "https://foundrymakes-ss.squarespace.com/template.git"
  },
  "author": "Jason Spidle",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.23.6",
    "parallelshell": "^3.0.2"
  }
}
