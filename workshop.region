          <squarespace:block-field id="workshop__deliverables-header" columns="12" lock-layout="true"/>
        </div>
        <div class="page__subheadline util__text-white">
          <squarespace:block-field id="workshop__deliverables-description" columns="12" lock-layout="true"/>
        </div>
